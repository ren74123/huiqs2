.privacy-container {
  padding: 20px;
  font-size: 16px;
  line-height: 1.6;
}

/* 适配markdown解析后的样式 */
.privacy-container h1 {
  font-size: 22px;
  margin: 20px 0 15px;
  font-weight: bold;
}

.privacy-container h2 {
  font-size: 18px;
  margin: 15px 0 10px;
  font-weight: bold;
}

.privacy-container p {
  margin: 10px 0;
}

.privacy-container ul {
  padding-left: 20px;
}